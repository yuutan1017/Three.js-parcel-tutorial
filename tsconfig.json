{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "types": ["node"],
    "jsx": "react",
    "lib": ["ES5", "ES6", "DOM"],
    "allowJs": true, // jsコンパイル用
    "checkJs": true, // js簡易型チェック
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "./dist/",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "noImplicitAny": false,
  }
}
